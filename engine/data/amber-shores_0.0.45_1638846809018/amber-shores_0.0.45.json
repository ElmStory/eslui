  "events": {
    "080c450b-016a-41e3-9492-96b09de09017": {
      "choices": [],
      "content": "[{\"type\":\"paragraph\",\"children\":[{\"text\":\"The dimly lit room is covered in dust, cobwebs and ice.\"}]},{\"type\":\"paragraph\",\"children\":[{\"text\":\"You are afraid, barely noticing the steam exiting your mouth.\"}]},{\"type\":\"paragraph\",\"children\":[{\"text\":\"Howling wind creaks the ancient wood.\"}]}]",
      "composer": {
        "sceneMapPosX": -91,
        "sceneMapPosY": 741.5
      },
      "ending": false,
      "id": "080c450b-016a-41e3-9492-96b09de09017",
      "persona": [
        "d65f167c-afa8-4590-83e3-84fc25200433",
        "NEUTRAL",
        null
      ],
      "sceneId": "13048caf-4075-4d4c-ace5-acf984eed1f4",
      "tags": [],
      "title": "Intro",
      "type": "CHOICE",
      "updated": 1638845991513
    },
    "09a1dab3-b83d-405e-9521-0f5336ce73b2": {
      "choices": [],
      "content": "[{\"type\":\"paragraph\",\"children\":[{\"text\":\"Quickly, you feel around for the lamp.\"}]},{\"type\":\"paragraph\",\"children\":[{\"text\":\"Pulling down on the gold chain, the dim red haze returns.\"}]},{\"type\":\"paragraph\",\"children\":[{\"text\":\"The room has changed.\"}]}]",
      "composer": {
        "sceneMapPosX": 1131.9618953919821,
        "sceneMapPosY": 679.3735419931475
      },
      "ending": false,
      "id": "09a1dab3-b83d-405e-9521-0f5336ce73b2",
      "sceneId": "fa2ecaf4-7746-4cdf-9bc8-0992738a7db3",
      "tags": [],
      "title": "Attempt to turn on the light",
      "type": "CHOICE",
      "updated": 1635994560224
    },
    "0ebfa65d-bfa6-45ed-abbe-55d457bc76bf": {
      "choices": [],
      "content": "[{\"type\":\"paragraph\",\"children\":[{\"text\":\"In shock, you snap your hand back, pulling the chain with you.\"}]},{\"type\":\"paragraph\",\"children\":[{\"text\":\"The lamp crashes to the floor, extinguishing the light.\"}]}]",
      "composer": {
        "sceneMapPosX": 1375.9618953919821,
        "sceneMapPosY": 679.3735419931475
      },
      "ending": false,
      "id": "0ebfa65d-bfa6-45ed-abbe-55d457bc76bf",
      "sceneId": "fa2ecaf4-7746-4cdf-9bc8-0992738a7db3",
      "tags": [],
      "title": "Breaking the lamp",
      "type": "CHOICE",
      "updated": 1635994560267
    },
    "1f02125d-f06c-4ddd-b369-38611e764490": {
      "choices": [],
      "content": "[{\"type\":\"paragraph\",\"children\":[{\"text\":\"\\\"How did I get here?\\\" you whisper.\"}]},{\"type\":\"paragraph\",\"children\":[{\"text\":\"Carefully looking around, you realize the absence of windows.\"}]},{\"type\":\"paragraph\",\"children\":[{\"text\":\"There are no doors.\"}]}]",
      "composer": {
        "sceneMapPosX": 162,
        "sceneMapPosY": 740
      },
      "ending": false,
      "id": "1f02125d-f06c-4ddd-b369-38611e764490",
      "persona": [
        "4719f708-7995-4cf1-9c1a-f974880d97c6",
        "NEUTRAL",
        "88bb5737-66a3-4525-baf2-3e586781d5c8"
      ],
      "sceneId": "13048caf-4075-4d4c-ace5-acf984eed1f4",
      "tags": [],
      "title": "Looking around",
      "type": "CHOICE",
      "updated": 1638845978775
    },
    "28a637df-584f-4ff7-9969-5e633da5d6e8": {
      "choices": [
        "bd686644-a515-48a0-b61c-85fa1423537f"
      ],
      "content": "[{\"type\":\"paragraph\",\"children\":[{\"text\":\"As your breathing quickens, panic begins to swell in your throat.\"}]},{\"type\":\"paragraph\",\"children\":[{\"text\":\"You feel the space around you closing in.\"}]}]",
      "composer": {
        "sceneMapPosX": 878.3003487735207,
        "sceneMapPosY": 659.3735419931475
      },
      "ending": false,
      "id": "28a637df-584f-4ff7-9969-5e633da5d6e8",
