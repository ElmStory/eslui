      "type": "CHOICE",
      "updated": 1635994554360
    },
    "569d12c3-7050-49e0-891e-9fde24167822": {
      "choices": [],
      "content": "[{\"type\":\"paragraph\",\"children\":[{\"text\":\"Thank you for playing this preview of Amber Shores.\"}]},{\"type\":\"paragraph\",\"children\":[{\"text\":\"The first chapter will be available December 2021.\"}]}]",
      "composer": {
